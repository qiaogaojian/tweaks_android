<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200pt"
    android:height="200pt"
    android:viewportWidth="200"
    android:viewportHeight="200">
    <!-- android:width / android:height  VectorDrawable 的实际大小-->
    <!-- android:viewportWidth / android:viewportHeight  画布的大小-->
    <!-- 大写字母代表绝对路径，小写的字母代表相对路径-->
    <!-- C/c 表示三阶贝赛尔曲线的命令字母，参数为(x1,y1 x2,y2 x,y)，其中 x1,y1 为第一个控制点，x2,y2 为第二个控制点，x,y 为绘制的结束点。-->

    <path
        android:name="triangle"
        android:fillColor="#F92671"
        android:pathData="M 0,100
                          L 100,100
                          L 100,0
                          A 100,100 0,1,1 0,100
                          " />

</vector>